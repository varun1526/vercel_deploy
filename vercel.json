{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/products",
        "dest": "index.js"
      },
      {
        "src": "/products/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/add",
        "dest": "dist/index.html"
      },
      {
        "src": "/",
        "dest": "dist/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "dist/$1"
      },
      {
        "src":"/api/products/.*",
        "dest":"dist/../index.js"
      }
    ]
  }